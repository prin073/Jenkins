pipeline {
    agent any
    stages {
        stage('1') {
            steps {
                sh 'echo "Hello World 1"'
            }
        }
        stage('2') {
            steps {
                sh 'echo "Hello World 2"'
            }
        }
        stage('3') {
            steps {
                sh 'echo "Hello World 3"'
            }
        }
    }
}
