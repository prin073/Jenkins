@Library("shared-library") _

pipeline {
        agent any
        
        stages {
            stage ("HelloWorldList") {
                steps {
                  listIterate(["Prince", "Inaaya", 1, 2])
                  listAdd(["Prince", "Inaaya"], "Mousami")
                  listRemove(["Prince", "Inaaya", "Mousami"], "Mousami")
                }
            
            }
        
        }
}