@Library("shared-library") _

pipeline {
        agent any
        
        stages {
            stage ("List") {
                steps {
                  listIterate(["Prince", "Inaaya", 1, 2])
                  listAdd(["Prince", "Inaaya"], "Mousami")
                  listAddOperator("Mousami", ["Prince", "Inaaya"])
                  listRemove("Mousami", ["Prince", "Inaaya", "Mousami"])
                }
            
            }
        
        }
}