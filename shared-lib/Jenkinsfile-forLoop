@Library("shared-library") _

pipeline {
        agent any
        
        stages {
            stage ("ForLoop") {
                steps {
                
                    forEach(["Prince", "Inaaya"])
                    forLoop(["Prince", "Inaaya"])
                    forLoopRange()
                    forLoopDownTo()  //bypassed CPS
                    forLoopClassic() //No CPS bypass
                    forLoopStep()
                }
            
            }
        
        }
}