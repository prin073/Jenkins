@Library("shared-library") _

pipeline {
        agent any
        
        stages {
            stage ("Map") {
                steps {
                
                    map(name: "Prince", weekOfDay: "Thursday")
                    map(weekOfDay: "Thursday", name: "Prince") // order doesn't matter
                    mapIterate(weekOfDay: "Thursday", name: "Prince")
                }
            
            }
        
        }
}